{
  "name": "@storevent/storevent",
  "version": "0.0.10",
  "description": "Storevent is a framework that simplify event sourcing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "all": "npm run test && npm run fix && npm run lint && npm run check-type && npm run build",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts",
    "fix": "prettier src --write && eslint . --ext .ts --fix",
    "check-type": "tsc --noEmit"
  },
  "keywords": [
    "event",
    "sourcing",
    "event sourcing",
    "cqrs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SachaCR/storevent.git"
  },
  "author": "SachaCR",
  "bugs": {
    "url": "git+https://github.com/SachaCR/storevent.git"
  },
  "homepage": "https://github.com/SachaCR/storevent/blob/main/packages/storevent/README.md",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.18.0",
    "npm": "^10.8.2"
  }
}
