{
  "name": "@storevent/storevent-pg",
  "version": "0.0.1",
  "description": "Postgres implementation of storevent library interface",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "all": "npm run test && npm run fix && npm run lint && npm run check-type && npm run build",
    "build": "tsc",
    "test": "jest --passWithNoTests --runInBand  --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "fix": "prettier src --write && eslint . --ext .ts --fix",
    "check-type": "tsc --noEmit",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SachaCR/storevent.git"
  },
  "keywords": [
    "event",
    "sourcing"
  ],
  "dependencies": {
    "@storevent/storevent": "*",
    "pg": "^8.13.0",
    "pg-format": "^1.0.4"
  },
  "author": "Sacha Clerc-Renaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SachaCR/storevent/issues"
  },
  "homepage": "https://github.com/SachaCR/storevent#readme",
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.6",
    "@types/pg": "^8.11.10",
    "@types/pg-format": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "config": "^3.3.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
